[Unit]
Description=Get PSC Tokens
Documentation=https://github.com/vmware/vic-product/installer

[Path]
PathExists=/etc/vmware/psc/admiral/psc-config.properties

[Path]
PathChanged=/etc/vmware/psc/admiral/psc-config.properties
